{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/get-elo",
      "dest": "/server.js",
      "methods": ["GET"]
    },
    {
      "src": "/*",
      "dest": "/server.js",
      "methods": ["GET"]
    },
    {
      "src": "/get-elo",
      "dest": "/server.js",
      "methods": ["POST"]
    }
  ]}
